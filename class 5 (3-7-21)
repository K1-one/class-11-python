#question 1 write a program to input a number to get output as the multiplication
#table

a = int(input("enter a no. "))
print('%d * 1 = %d' %(a, a*1))
print('%d * 2 = %d' %(a, a*2))
print('%d * 3 = %d' %(a, a*3))
print('%d * 4 = %d' %(a, a*4))
print('%d * 5 = %d' %(a, a*5))
print('%d * 6 = %d' %(a, a*6))
print('%d * 7 = %d' %(a, a*7))
print('%d * 8 = %d' %(a, a*8))
print('%d * 9 = %d' %(a, a*9))
print('%d * 10 = %d' %(a, a*10))


